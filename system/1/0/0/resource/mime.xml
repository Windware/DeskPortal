<root>
	<type ext="png" mime="image/png" compress="0"/>
	<type ext="jpg" mime="image/jpeg" compress="0"/>
	<type ext="gif" mime="image/gif" compress="0"/>
	<type ext="ico" mime="image/vnd.microsoft.icon" compress="0"/>

	<type ext="txt" mime="text/plain" compress="1"/>
	<type ext="xml" mime="text/xml" compress="1"/>

	<type ext="html" mime="text/html" compress="1"/>
	<type ext="js" mime="text/javascript" compress="1"/>
	<type ext="css" mime="text/css" compress="1"/>

	<!-- Specifically using 'text' instead of 'application' for system integrity purpose -->
	<type ext="json" mime="text/json" compress="1"/>
</root>
